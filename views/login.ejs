<!DOCTYPE html>
<html>
<head>
    <title>Itechart.Connect</title>

    <link rel='stylesheet' href='/javascript/bower_components/bootstrap/dist/css/bootstrap.min.css' />
    <link rel='stylesheet' href='/javascript/bower_components/bootstrap/dist/css/bootstrap-theme.min.css' />
    <link rel='stylesheet' href='/javascript/bower_components/angular/angular-csp.css' />
    <link rel='stylesheet' href='/stylesheets/base.css' />
</head>
<body>
<header>
    <img class="main-logo" src="/images/logo.png"/>
</header>

<div id="main-content">
    <form method="post" id="login-form">
        <div class="form-group">
            <label for="username">Email address</label>
            <input type="text" class="form-control" name="username" placeholder="Enter Username">
        </div>
        <div class="form-group">
            <label for="password">Password</label>
            <input type="password" class="form-control" name="password" placeholder="Password">
        </div>
    </form>
    <button class="btn btn-default submit-login">Login</button>
</div>

<script src="/javascript/bower_components/jquery/dist/jquery.min.js"></script>
<script src="/javascript/login/login.js"></script>

<footer>
</footer>

</body>
</html>